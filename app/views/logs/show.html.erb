<p style="color: green"><%= notice %></p>

<%= render @log %>

<div>
  <% if current_user.is_admin? %>
    <%= link_to "Edit this log", edit_log_path(@log) %> |
  <% end %>
  <%= link_to "Back to logs", logs_path %>

  <% if current_user.is_admin? %>
    <%= button_to "Destroy this log", @log, method: :delete %>
  <% end %>
</div>
